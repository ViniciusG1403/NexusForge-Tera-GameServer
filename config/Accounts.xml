	<!-- premium user money rate (2.0) -->
	<set name="ACCOUNT_PREMIUM_MONEY_RATE" value="2.0" />
	<!-- premium user item drop rate (2.0)-->
	<set name="ACCOUNT_PREMIUM_DROP_RATE" value="2.0" />

	<!-- premium user quest rate (2.0)-->
