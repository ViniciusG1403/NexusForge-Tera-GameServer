	<set name="ACCOUNT_ONLY_PAID" value="false" />
	
	<!-- enable premium user bonus exp feature (true) -->
	<set name="ACCOUNT_PREMIUM_EXP" value="true" />
	<!-- enable premium user bonus money feature (true) -->
	<set name="ACCOUNT_PREMIUM_MONEY" value="true" />
